    "date-fns": "^2.19.0",
    "express": "^4.18.1",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.8",
    "prisma": "3.7.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.1",
    "zod": "3.11.6"
  }
}
