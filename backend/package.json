{
  "name": "dag-app",
  "version": "22.8.0",
  "main": "dist/index.js",
  "license": "AGPLv3",
  "scripts": {
    "generate": "npx prisma generate && cp -r ./src/prisma/clients ./dist/prisma/clients",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "dev": "npm run build && node .",
    "start": "node ./dist/src",
    "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
    "docker:build": "docker build . --tag ${npm_package_name}:latest --tag ${npm_package_name}:${npm_package_version} --target=final"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/form-data": "^2.5.0",
    "@types/fs-extra": "^9.0.11",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@prisma/client": "3.7.0",
    "@types/nodemailer": "^6.4.5",
    "chalk": "^4.1.0",
